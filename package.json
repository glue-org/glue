{
  "name": "fpd",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "predev": "dfx canister create backend && dfx build backend && npm run copy:types && dfx deploy --mode=reinstall backend",
    "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --delete src/declarations"
  },
  "dependencies": {
    "@dfinity/agent": "^0.10.0",
    "@dfinity/auth-client": "^0.10.0",
    "@dfinity/authentication": "^0.10.0",
    "@dfinity/candid": "^0.10.0",
    "@dfinity/identity": "^0.10.0",
    "@dfinity/principal": "^0.10.0",
    "daisyui": "^2.15.2",
    "ic-stoic-identity": "^2.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "next",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^16.11.6",
    "autoprefixer": "^10.4.2",
    "buffer": "^6.0.3",
    "cssnano": "^5.0.17",
    "postcss": "^8.4.6",
    "postcss-load-config": "^3.1.1",
    "svelte": "^3.42.5",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^3.0.18",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.7"
  }
}
